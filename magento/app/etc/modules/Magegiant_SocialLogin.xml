<config>
    <modules>
        <!-- Facebook -->
        <Magegiant_SocialLogin>
            <active>true</active>
            <codePool>community</codePool>
            <depends><Magegiant_Community/></depends>
        </Magegiant_SocialLogin>

        <Magegiant_GgLogin>
            <active>true</active>
            <codePool>community</codePool>
            <depends>
                <Magegiant_SocialLogin/>
            </depends>
        </Magegiant_GgLogin>

        <Magegiant_TwLogin>
            <active>true</active>
            <codePool>community</codePool>
             <depends>
                <Magegiant_SocialLogin/>
            </depends>
        </Magegiant_TwLogin>

        <Magegiant_WpLogin>
            <active>true</active>
            <codePool>community</codePool>
        </Magegiant_WpLogin>

        <Magegiant_YhLogin>
            <active>true</active>
            <codePool>community</codePool>
            <depends>
                <Magegiant_SocialLogin/>
            </depends>
        </Magegiant_YhLogin>

        <Magegiant_LiveLogin>
            <active>true</active>
            <codePool>community</codePool>
            <depends>
                <Magegiant_SocialLogin/>
            </depends>
        </Magegiant_LiveLogin>
    </modules>
</config>
