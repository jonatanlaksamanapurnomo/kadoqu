<?xml version="1.0"?>
<config>
	  <sections>
		
		<carriers  translate="label" module="proaongkir">            
					<groups>
				      <ongkir translate="label"> 
					  <label>Magento Pro Rajaongkir</label>
					  <frontend_type>text</frontend_type>
					  <sort_order>0</sort_order>
					  <show_in_default>1</show_in_default>
					  <show_in_website>1</show_in_website>
					  <show_in_store>1</show_in_store>
					  <model>proaongkir/carrier_ongkir</model>
				       <fields>                        
				         <active translate="label">
                            <label>Enabled</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </active>
                        <title translate="label">
                            <label>Title</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>2</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </title>
                        <name translate="label">
                            <label>Method Name</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>3</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </name>
           
                        <sort_order translate="label">
                            <label>Sort Order</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>4</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <sort_order>100</sort_order>
                        </sort_order>
						
						
						<apiurl translate="label">
                            <label>Api URL</label>
							<frontend_type>text</frontend_type>
                            <sort_order>5</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							
                          </apiurl>
                          <apikey translate="label">
                            <label>Api Key</label>
							<frontend_type>text</frontend_type>
                            <sort_order>6</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							<comment>Get Your API Key at rajproaongkir.com</comment>
                          </apikey>
                          <origin translate="label">
                            <label>City Origins</label>
                            <frontend_type>select</frontend_type>
                            <source_model>Ndee_Proproaongkir_Model_System_Config_Source_City</source_model>
                            <sort_order>7</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							<comment>Shipping (you have to enter api key first to get city list)</comment>
                          </origin>
                          <kurir translate="label">
                            <label>Shipping Couriers</label>
                            <frontend_type>multiselect</frontend_type>
                            <source_model>Ndee_Proproaongkir_Model_System_Config_Source_Kurir</source_model>
                            <sort_order>8</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							<comment>Select Courires</comment>
                          </kurir>
						  
						  <disablesvr translate="label">
                            <label>Disable Service</label>
                            <frontend_type>multiselect</frontend_type>
                            <source_model>Ndee_Proproaongkir_Model_System_Config_Source_Serpis</source_model>
                            <sort_order>9</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							<comment>Select shipping service you want to disable</comment>
                          </disablesvr>
						  
						  <jquery translate="label">
                            <label>Include jQuery Lib</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>999</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                          </jquery>
						  
						  <changeprice translate="label">
                            <label>Add or Substract Shipping Price by </label>
                            <frontend_type>text</frontend_type>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							<comment><![CDATA[example: +5% , +5000 , -5% , -5000 , leave it empty if not necessary, plus(+) or minus (-) sign is MANDATORY to use this feature ]]></comment>
                        </changeprice>
						
						<disablecached translate="label">
                            <label>Disabled cached shipping rates</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>11</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </disablecached>
						
						  <freeshipping_city translate="label">
                            <label>Free Shipping Location</label>
                            <frontend_type>multiselect</frontend_type>
                            <source_model>Ndee_Proproaongkir_Model_System_Config_Source_City</source_model>
                            <sort_order>12</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							<comment><![CDATA[Shipping (you have to enter api key first to get city list) <style>#carriers_ongkir_freeshipping_city{width:520px}</style>]]></comment>
                          </freeshipping_city>
						  
						  <freeshipping_min_subtot translate="label">
                            <label>Minimum Subtotal</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>13</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							<comment><![CDATA[number only, shopping cart subtotal before discount ]]></comment>
                         </freeshipping_min_subtot>
						 
						<freeshipping_text translate="label">
                            <label>Free Shipping Text</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>14</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							
                         </freeshipping_text>
						
						
						
					   </fields>
					   </ongkir>
					</groups>
		</carriers>
	  </sections>
</config>
